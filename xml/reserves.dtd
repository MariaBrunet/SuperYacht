            <!ELEMENT llinatge1 (#PCDATA)>
            <!ELEMENT llinatge2 (#PCDATA)>
            <!ELEMENT dni (#PCDATA)>
            <!ELEMENT telf (#PCDATA)>
            <!ELEMENT direccio (#PCDATA)>
        <!ELEMENT data (entrada, sortida)>
            <!ELEMENT entrada (#PCDATA)>
            <!ELEMENT sortida (#PCDATA)>
